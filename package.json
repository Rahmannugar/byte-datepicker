{
  "name": "byte-datepicker",
  "version": "0.1.1",
  "description": "A lightweight, elegant, and highly customizable React datepicker componentlibrary with support for both month/year and full date selection.",
  "author": "Rahman Nugar",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rahmannugar/byte-datepicker.git"
  },
  "homepage": "https://github.com/Rahmannugar/byte-datepicker#readme",
  "bugs": {
    "url": "https://github.com/Rahmannugar/byte-datepicker/issues"
  },
  "keywords": [
    "react",
    "datepicker",
    "date",
    "picker",
    "calendar",
    "typescript",
    "component",
    "ui",
    "month-picker",
    "year-picker"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --no-splitting",
    "build:css": "echo 'Remember to manually copy src/index.css to dist/styles.css'",
    "dev": "vite",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0"
  }
}
